<UIView xml="../Components/Background.xml">
    <UIView xml="../Components/SafeArea.xml">
        <UITableView
            backgroundColor="#fff"
            outlet="tableView"
            separatorStyle="none"
            rowHeight="64"
            height="parent.height"
            style="plain">
            <UITableViewCell
                reuseIdentifier="cell"
                selectionStyle="{UITableViewCell.SelectionStyle.none}"
                textLabel.text="{title}"
                />
        </UITableView>
        <UIButton
            title="blob"
            width="parent.width - 20"
            left="10"
            backgroundColor="UIColor.gray"
            touchUpInside="alert"
            titleColor="black"
            top="previous.bottom - 40"
            height="30"
            />
    </UIView>
</UIView>
