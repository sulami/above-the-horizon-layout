<UIView xml="Background.xml">
    <UIView xml="SafeArea.xml">
        <UITableView
            style="plain"
            >
            <UITableViewCell
                reuseIdentifier="text"
                >
                <UIStackView
                    axis="horizontal"
                    alignment="{UIStackView.Alignment.center}"
                    distribution="{UIStackView.Distribution.equalSpacing}"
                    left="10"
                    right="100% - 10"
                    height="100%">
                    <UILabel
                        text="{labelText}"
                        />
                    <UILabel
                        text="{contentText}"
                        font="12"
                        />
                </UIStackView>
            </UITableViewCell>
        </UITableView>
    </UIView>
</UIView>
