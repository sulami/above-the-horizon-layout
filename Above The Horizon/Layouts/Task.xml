<UIView xml="Background.xml">
    <UIView xml="SafeArea.xml">
        <UITableView
            style="plain"
            >
            <UITableViewCell
                reuseIdentifier="text"
                textLabel.text="{labelText}"
                >
                <UILabel
                    text="{contentText}"
                    font="12"
                    />
            </UITableViewCell>
        </UITableView>
    </UIView>
</UIView>
